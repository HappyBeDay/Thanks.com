<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org./dtd/mybatis-3-mapper.dtd">
<mapper namespace="sist.com.vo.board">

	<insert id="insertBoard" parameterType="board">
		insert into
		board(BOARDCODE,MEMBERCODE,BOARDTITLE,BOARDCONTENT,BOARDPIC,
		BOARDDATE,BOARDHIT,BOARDLIKE,BOARDTYPECODE,BOARDNUM,REPLYGROUPCODE)
		values(#{boardcode},#{membercode},#{boardtitle},#{boardcontent},#{boardpic},
		#{boarddate},#{boardhit},#{boardlike},#{boardtypecode},#{boardnum},#{replygroupcode})
	</insert>
	
	
	

</mapper>