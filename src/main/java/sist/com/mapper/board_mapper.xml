<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org./dtd/mybatis-3-mapper.dtd">
<mapper namespace="sist.com.vo.board">

	<select id="selectBoardAll" resultType="boardList">
		select * from board
	</select>
	<select id="selectBoardGeneral" resultType="boardList">
		select * from board
		where boardtypecode=1
	</select>
	<select id="selectBoardCare" resultType="boardList">
		select * from board
		where boardtypecode=2
	</select>
</mapper>