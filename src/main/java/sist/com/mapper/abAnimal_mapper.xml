<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org./dtd/mybatis-3-mapper.dtd">
<mapper namespace="sist.com.vo.breed">
	<sql id="table">AbandonedAnimal</sql>
	<sql id="pk">abanimalCode</sql>
	<sql id="columnsAPI"></sql>
	<sql id="insertColumnsAPI">#{}, #{}</sql>

	<!-- insert -->
	<insert id="insertAbAnimalFromApi" parameterType="java.util.Map">
		insert into 
		<include refid="table" />
		(<include refid="columnsAPI" />) 
		values 
		(<include refid="insertColumnsAPI" />) 
	</insert>

	<!-- select -->
	<select id="selectAbAnimalAll" resultType="abShelter">
		select * from
		<include refid="table"></include>
	</select>

	<select id="selectAbAnimal_PK" resultType="java.lang.Long">
		select
		<include refid="pk" />
		from
		<include refid="table" />
	</select>

</mapper>